{
  "name": "docker-ts-node",
  "version": "1.0.0",
  "description": "Node.js with TypeScript, dockerized",
  "main": "dist/app.js",
  "scripts": {
    "dev": "./node_modules/.bin/ts-node-dev --respawn --transpile-only --no-notify -- app/app.ts",
    "debug": "./node_modules/.bin/ts-node-dev --respawn --transpile-only --no-notify --inspect=0.0.0.0:3001 -- app/app.ts",
    "test": "./node_modules/.bin/jest",
    "coverage": "./node_modules/.bin/jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
